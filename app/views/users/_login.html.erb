Log In:

<%= form_for @user, url: "/users/login" do |f| %>
  <%= f.label :username, "Username: " %>
  <%= f.text_field :username %><br>
  <%= f.label :password, "Password: " %>
  <%= f.password_field :password %><br>
  <%= f.submit "Log In" %>
<% end %><br><br>

<% if flash[:login_error] %>
  <%= flash[:login_error]%>
<% end %>